{
  "name": "Trello Custom Fields Colored",
  "version": "1.0",
  "description": "Color coordinates Trello Custom Fields by value",
  "permissions": ["storage", "https://trello.com/"],
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"], 
      "js": ["color-fields.js"],
      "css": ["color-fields.css"]
    }
  ],
  "options_page": "options.html",
  "manifest_version": 2
}